#include <stdio.h>

int main() {
    int a, b, temp;

    printf("Enter two numbers: ");
    scanf("%d %d", &a, &b);

    printf("\nBefore swapping: a = %d, b = %d\n", a, b);

   temp = a;
    a = b;
    b = temp;
    printf("After swapping (method 1 - temp variable): a = %d, b = %d\n", a, b);

   
    a = a + b;
    b = a - b;
    a = a - b;
    printf("After swapping (method 2 - arithmetic): a = %d, b = %d\n", a, b);

    a = a ^ b;
    b = a ^ b;
    a = a ^ b;
    printf("After swapping (method 3 - XOR): a = %d, b = %d\n", a, b);

   int *p1 = &a, *p2 = &b, temp2;
    temp2 = *p1;
    *p1 = *p2;
    *p2 = temp2;
    printf("After swapping (method 4 - pointers): a = %d, b = %d\n", a, b);

    return 0;
}
